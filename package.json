{
	"name": "legaltrack",
	"version": "0.1.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"db:drift": "dotenv -e .env.local -- drizzle-kit diff:pg --schema src/infra/db/schema --out src/infra/db/migrations",
		"db:generate": "dotenv -e .env.local -- drizzle-kit generate",
		"db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
		"db:studio": "dotenv -e .env.local -- drizzle-kit studio",
		"build": "next build",
		"start": "next start",
		"prepare": "husky",
		"supabase:types": "supabase gen types typescript --local --schema public > src/libs/supabase/database.ts",
		"check": "biome check .",
		"fix": "biome check --write .",
		"ci": "biome check --error-on-warnings .",
		"format": "biome format --write ."
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx}": [
			"biome check --write"
		],
		"src/**/*.{json,md}": [
			"biome format --write"
		]
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
